# API Keys (you only need one of these)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
XAI_API_KEY=your-xai-api-key-here

# OpenAI Model Configuration
OPENAI_EMBEDDING_MODEL=text-embedding-3-large
OPENAI_ANALYSIS_MODEL=gpt-4o
OPENAI_REPORT_MODEL=gpt-4o

# Anthropic Model Configuration
ANTHROPIC_ANALYSIS_MODEL=claude-3-7-sonnet-20240620
ANTHROPIC_REPORT_MODEL=claude-3-7-sonnet-20240620

# X AI (Grok) Model Configuration
XAI_ANALYSIS_MODEL=grok-2
XAI_REPORT_MODEL=grok-2

# Optional: GitHub API key for repo operations
# GITHUB_API_KEY=your-github-api-key-here

# Audit Logging
ENABLE_AUDIT_LOGGING=true